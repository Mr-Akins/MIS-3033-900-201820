<Window x:Class="GOT_JSON.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GOT_JSON"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Loaded="Window_Loaded">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="https://hips.hearstapps.com/hmg-prod.s3.amazonaws.com/images/peter-dinklage-look-1536330273.jpg" 
                        Stretch="UniformToFill"/>
        </Grid.Background>
        <TextBlock x:Name="quote" Margin="512,10,10,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="301" Foreground="#FFEE2020"/>
    </Grid>
</Window>
